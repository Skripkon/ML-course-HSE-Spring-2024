def get_a_hero_with_a_maximum_number_of_legs(heroes):
    max_legs = 0
    hero_index = None
    for index, hero in enumerate(data):
        if hero['legs'] > max_legs:
            max_legs = hero['legs']
            hero_index = index
    return heroes[hero_index]


